<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.getfit.getfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_getfit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_getfit_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GetFit">
        <activity android:name="com.getfit.getfit.Chronometer"></activity>
        <activity android:name="com.getfit.getfit.StepActivity" />
        <activity android:name="com.getfit.getfit.JumpActivity" />
        <activity android:name="com.getfit.getfit.TimerActivity" />
        <activity android:name="com.getfit.getfit.StopwatchActivity" />
        <activity android:name="com.getfit.getfit.RegisterActivity" />
        <activity android:name="com.getfit.getfit.LoginActivity" />
        <activity android:name="com.getfit.getfit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACTIVITY_RECOGNITION" />

    <uses-permission android:name="android.permission.VIBRATE" />

</manifest>